a = int(input("Digite o valor do numerador: "))
b = int(input("Digite o valor do denominador: "))

q = 0
r = a

while(r > b):
    q += 1
    r -= b

print(f"q = {q} \t r = {r}")a = int(input("Digite o valor do numerador: "))
